.page {
  background-color: #EEEEF4;
}
.swiper {
  height: 225px;
}
.swiper image {
  width: 100%;
}
.notify-container {
  background-color: #FFFFFF;
  padding: 25rpx;
  position: relative;
  margin-bottom: 20rpx;
}
.notify-container .notify-title {
  color: #3474FF;
  font-size: 34rpx;
  margin-bottom: 10rpx;
}
.notify-container .notify-title .notify-icon {
  width: 38rpx;
  margin-right: 5rpx;
  vertical-align: -8rpx;
}
.notify-container .notify-content {
  font-size: 30rpx;
  color: #333;
}
.notify-container .notify-content:before {
  display: inline-block;
  content: "";
  width: 12rpx;
  height: 12rpx;
  border-radius: 6rpx;
  background-color: red;
  margin-left: 18rpx;
  margin-right: 15rpx;
  vertical-align: 3rpx;
}
.notify-container .more-icon {
  position: absolute;
  right: 5rpx;
  top: 50rpx;
  width: 60rpx;
}
.nav-container {
  background-color: #FFFFFF;
  padding: 25rpx 40rpx;
  margin-bottom: 20rpx;
}
.nav-container .nav-row {
  display: flex;
  justify-content: space-between;
  margin-bottom: 0;
}
.nav-container .nav-row .nav {
  text-align: center;
}
.nav-container .nav-row .nav .icon {
  width: 80rpx;
}
.nav-container .nav-row .nav .name {
  display: block;
  font-size: 30rpx;
  color: #333;
}
.calendar-container {
  margin-bottom: 20rpx;
}

